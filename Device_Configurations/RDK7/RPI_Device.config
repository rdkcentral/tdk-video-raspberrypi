FIREBOLT_COMPLIANCE_USE_AUTOVIDEO_FOR_FPSDISPLAYSINK = no
#Check Audio frames from audiosink "stats" property
FIREBOLT_COMPLIANCE_CHECK_AUDIO = yes
#Audio start point to notify pipeline where video starts after underrun occurs (only for underflow tests)(default value is set as per TDK underflow stream)
FIREBOLT_COMPLIANCE_UNDERFLOW_AUDIO_START_POINT = 240
#Audio End point to notify pipeline where underrun occurs (only for underflow tests)(default value is set as per TDK underflow stream)
FIREBOLT_COMPLIANCE_UNDERFLOW_AUDIO_END_POINT = 10
#AVSync logic enabled in DUT or not (gstreamer playback stops if audio not present in play without audio test)
FIREBOLT_COMPLIANCE_AVSYNC_ENABLED = no
#Validate playback upto milliseconds level
FIREBOLT_COMPLIANCE_VALIDATE_FULL_PLAYBACK = yes
#Validate using appsrc-playbin pipeline
FIREBOLT_COMPLIANCE_USE_APPSRC = no
#Start westeros compositor before starting playback
FIREBOLT_COMPLIANCE_START_WESTEROS = no
#Create RDKShell display before starting playback
FIREBOLT_COMPLIANCE_CREATE_RDKSHELL_DISPLAY = no
#Platform config to enable 3x,4x fastforward testcases (yes/no)
